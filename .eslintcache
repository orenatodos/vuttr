>\n        <Button onClick={() => setModalIsShown(!modalIsShown)}>Cancel</Button>\n        <Button onClick={handleRemove}>Yes, remove</Button>\n      </div>\n    </S.Container>\n  );\n}\n","/home/orenatodev/www/github/public/vuttr/src/components/Confirmation/styles.ts",[],"/home/orenatodev/www/github/public/vuttr/src/utils/getValidationErrors.ts",[],"/home/orenatodev/www/github/public/vuttr/src/components/Tool/index.tsx",["110"],"import { useState } from 'react';\nimport { FiEdit, FiTrash } from 'react-icons/fi';\n\nimport Button from '../Button';\nimport Modal from '../Modal';\nimport Confirmation from '../Confirmation';\n\nimport { Tool as ToolItem } from '../../App';\n\nimport * as S from './styles';\n\ninterface ToolProps {\n  tool: ToolItem;\n  tools: ToolItem[];\n  setTools: any;\n}\n\nexport default function Tool(props: ToolProps) {\n  const { tool, tools, setTools } = props;\n\n  const [modalIsShown, setModalIsShown] = useState(false);\n\n  return (\n    <>\n      <S.Container>\n        <a href={tool.link}>{tool.title}</a>\n        <p>{tool.description}</p>\n        <ul>\n          {tool.tags.map((tag, index) => (\n            // eslint-disable-next-line\n            <li key={index}>#{tag}</li>\n          ))}\n        </ul>\n        <div>\n          <Button type=\"button\">\n            <FiEdit size={16} />\n          </Button>\n          <Button\n            type=\"button\"\n            className=\"remove\"\n            onClick={() => setModalIsShown(!modalIsShown)}\n          >\n            <FiTrash size={16} />\n          </Button>\n        </div>\n      </S.Container>\n      <Modal isShown={modalIsShown} hide={() => setModalIsShown(!modalIsShown)}>\n        <Confirmation\n          id={tool.id}\n          title={tool.title}\n          tools={tools}\n          setTools={setTools}\n          modalIsShown={modalIsShown}\n          setModalIsShown={setModalIsShown}\n        />\n      </Modal>\n    </>\n  );\n}\n","/home/orenatodev/www/github/public/vuttr/src